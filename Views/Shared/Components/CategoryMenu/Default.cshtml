@model IEnumerable<eShop.Data.Entities.Category>

<li class="dropdown">
    <a asp-controller="Event" asp-action="Index"
       class="dropdown-toggle nav-link text-dark" data-toggle="dropdown">Events<b class="caret"></b></a>
    <ul class="dropdown-menu">
        @foreach (var category in Model)
         {
            <li>
                <a class="nav-link text-dark" asp-controller="Event" asp-action="Index"
                  asp-route-category="@category.CategoryName">@category.CategoryName</a>
            </li>
         }
        <li class="dropdown-divider"></li>
        <li>
            <a class="nav-link text-dark" asp-controller="Event" asp-action="Index" asp-route-Category="">View all categoryies</a>
        </li>
    </ul>
</li>