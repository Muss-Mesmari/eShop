@model eShop.Presentation.ViewModels.EventViewModel


@{
    if (@Model.Events.Any())
    {
        <div class="card-deck">
            @foreach (var item in Model.Events)
            {
                <partial name="_EventCard" model="item" />
            }
        </div>
    }
    else
    {
        <div class="container-fluid">
            <p class="text-danger text-center"> @Model.NotFoundSearchedBarMessage</p>
        </div>
    }
}